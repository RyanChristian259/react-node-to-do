{
  "name": "react-node-to-do",
  "version": "1.0.0",
  "description": "A full stack app with Node.js and React",
  "main": "index.js",
  "scripts": {
    "build": "webpack -p",
    "deploy": "npm run build && git commit -a -m deployment && git push heroku master",
    "dev": "webpack-dev-server --open",
    "server" : "babel-node src/server/server",
    "server-test" : "babel-node src/server/server.spec",
    "server-dev": "nodemon src/server --exec babel-node src/server",
    "start-dev": "concurrently \"npm run server-dev\" \"npm run dev\"",
    "server-spec": "babel-node src/server/server.spec",
    "start": "npm run server",
    "initialize-db": "babel-node src/server/initialize-db"
  },
  "keywords": [
    "react",
    "redux",
    "express",
    "mongo"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "^4.1.3",
    "eslint": "^6.6.0",
    "history": "^4.7.2",
    "md5": "^2.2.1",
    "react": "^16.4.2",
    "react-dom": "^16.5.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.2",
    "uuid": "^3.3.2",
    "webpack": "^5.75.0"
  },
  "engines": {
    "node": "9.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-loader": "^8.3.0",
    "body-parser": "^1.18.3",
    "concurrently": "^4.0.1",
    "cors": "^2.8.4",
    "cross-env": "^5.2.0",
    "express": "^4.16.3",
    "highground": "^1.0.3-beta",
    "jest": "^29.3.1",
    "mongodb": "^3.1.10",
    "nodemon": "^1.18.4",
    "style-loader": "^3.3.1",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^3.1.7"
  }
}
